{
    "name": "inpsyde/user-table-showcase",
    "description": "A WordPress plugin that showcases the user data from a third party API into a table.",
    "license": "GPL-2.0+",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Nabi",
            "email": "artsanaat@gmail.com"
        }
    ],
    "support": {
        "src": "https://github.com/navidabdi/inpsyde",
        "issues": "https://github.com/navidabdi/inpsyde/issues"
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "brain/monkey": "^2",
        "inpsyde/php-coding-standards": "^1.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Inpsyde\\UserTableShowcase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Inpsyde\\UserTableShowcase\\Tests\\Unit\\": "tests/PHPUnit/Unit/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}